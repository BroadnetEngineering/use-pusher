{
  "name": "use-pusher",
  "version": "1.0.4",
  "description": "A wrapper around pusher-js for easy-as hooks in React.",
  "author": "@mayteio",
  "keywords": [
    "react",
    "react-hooks",
    "pusher",
    "channels",
    "pusher-js",
    "websockets",
    "realtime messaging"
  ],
  "license": "MIT",
  "repository": "@mayteio/use-pusher",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "types": "dts-bundle-generator -o ./dist/index.d.ts ./src/index.ts --external-imports=react,pusher-js",
    "docs": "typedoc --options ./typedoc.js ./src"
  },
  "dependencies": {
    "dequal": "^1.0.0",
    "invariant": "^2.2.4"
  },
  "peerDependencies": {
    "pusher-js": "^5.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "@testing-library/react": "^9.1.2",
    "@testing-library/react-hooks": "^2.0.1",
    "@types/invariant": "^2.2.30",
    "@types/jest": "^23.3.13",
    "@types/pusher-js": "^4.2.2",
    "@types/react": "^16.7.22",
    "cross-env": "^5.2.0",
    "gh-pages": "^2.0.1",
    "jest-fetch-mock": "^2.1.2",
    "pusher-js": "^5.0.0",
    "pusher-js-mock": "^0.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "react-test-renderer": "^16.9.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-dts": "^1.1.6",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.19.2",
    "rollup-plugin-url": "^2.1.0",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.15.0",
    "typescript": "^3.2.4"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "./src/index.ts",
      "./src/mocks.ts",
      "./src/types.ts"
    ]
  }
}
